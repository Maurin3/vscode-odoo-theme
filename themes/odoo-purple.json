    "name": "Odoo Purple",
    "tokenColors": [
        {
            "name": "Built-in constant",
            "scope": "constant.language",
            "settings": {
                "foreground": "#d1a9c7"
            }
        },
        {
            "name": "Class name",
            "scope": [
                "entity.name.class",
                "entity.name.type"
            ],
            "settings": {
                "fontStyle": "underline",
                "foreground": "#33fffc"
            }
        },
        {
            "name": "Comment",
            "scope": "comment",
            "settings": {
                "foreground": "#737373"
            }
        },
        {
            "name": "Function argument",
            "scope": "variable.parameter",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#E46F78"
            }
        },
        {
            "name": "Function name",
            "scope": "entity.name.function",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#d1a9c7"
            }
        },
        {
            "name": "Inherited class",
            "scope": "entity.other.inherited-class",
            "settings": {
                "fontStyle": "italic underline",
                "foreground": "#33fffc"
            }
        },
        {
            "name": "Invalid deprecated",
            "scope": "invalid.deprecated",
            "settings": {
                "foreground": "#F0F0F0"
            }
        },
        {
            "name": "Invalid",
            "scope": "invalid",
            "settings": {
                "fontStyle": "",
                "foreground": "#F0F0F0"
            }
        },
        {
            "name": "Keyword",
            "scope": "keyword",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#68cac8"
            }
        },
        {
            "name": "Library class/type",
            "scope": [
                "support.class",
                "support.type"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#c487b4"
            }
        },
        {
            "name": "Library constant",
            "scope": "support.constant",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#a3508d"
            }
        },
        {
            "name": "Library function",
            "scope": "support.function",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#d1a9c7"
            }
        },
        {
            "name": "Library variable",
            "scope": "support.other.variable",
            "settings": {
                "fontStyle": "",
                "foreground": "#a3508d"
            }
        },
        {
            "name": "Number",
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#e0adb1"
            }
        },
        {
            "name": "Storage type",
            "scope": "storage.type",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#e0adb1"
            }
        },
        {
            "name": "Storage",
            "scope": "storage",
            "settings": {
                "fontStyle": "",
                "foreground": "#5B899E"
            }
        },
        {
            "name": "String",
            "scope": "string",
            "settings": {
                "foreground": "#ad85a3"
            }
        },
        {
            "name": "Tag attribute",
            "scope": "entity.other.attribute-name",
            "settings": {
                "fontStyle": "",
                "foreground": "#efa9af"
            }
        },
        {
            "name": "Tag name",
            "scope": "entity.name.tag",
            "settings": {
                "fontStyle": "",
                "foreground": "#d1a9c7"
            }
        },
        // take 60-80% (w3schools)
        {
            "name": "User-defined constant",
            "scope": [
                "constant.character",
                "constant.other"
            ],
            "settings": {
                "foreground": "#2a8280"
            }
        },
        {
            "name": "Variable",
            "scope": "variable",
            "settings": {
                "fontStyle": "",
                "foreground": "#21B799"
            }
        }
    ],
    "type": "dark"
