    "name": "Odoo Light",
    "tokenColors": [
        {
            "name": "Built-in constant",
            "scope": "constant.language",
            "settings": {
                "foreground": "#f280d0"
            }
        },
        {
            "name": "Class name",
            "scope": [
                "entity.name.class",
                "entity.name.type"
            ],
            "settings": {
                "fontStyle": "underline",
                "foreground": "#188181"
            }
        },
        {
            "name": "Comment",
            "scope": "comment",
            "settings": {
                "foreground": "#6b72fa"
            }
        },
        {
            "name": "Function argument",
            "scope": "variable.parameter",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#848afb"
            }
        },
        {
            "name": "Function name",
            "scope": "entity.name.function",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#61387a"
            }
        },
        {
            "name": "Inherited class",
            "scope": "entity.other.inherited-class",
            "settings": {
                "fontStyle": "italic underline",
                "foreground": "#188181"
            }
        },
        {
            "name": "Invalid deprecated",
            "scope": "invalid.deprecated",
            "settings": {
                "foreground": "#020202"
            }
        },
        {
            "name": "Invalid",
            "scope": "invalid",
            "settings": {
